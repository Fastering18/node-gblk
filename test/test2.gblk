fungsi piramid(tinggi, kebalik)
    jika tinggi < 3 maka kembali nil tutup
    kebalik = kebalik atau salah
    lokal hasil = ""
    untuk i = 0 ke tinggi lakukan
        jika (i == 0 dan kebalik) maka 
            hasil = hasil + ("* " * (tinggi - i)) + "\n";
            lanjutkan
        tutup
        hasil = hasil + (" " * (kebalik ? i : (tinggi - i))) + ("* " * (kebalik ? (tinggi - i) : i)) + "\n"
    tutup
    kembali hasil
tutup

print(piramid(10) + piramid(10, benar))

